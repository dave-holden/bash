if command -v source; then
  echo "Source command found"
  source ./core/shell_env.sh
  source ./output/spinners.sh
  source ./output/colours.sh
  source ./output/output.sh
else
  echo "Source command Not found"
  . ./core/shell_env.sh
  . ./output/spinners.sh
  . ./output/colours.sh
  . ./output/output.sh
fi
echo